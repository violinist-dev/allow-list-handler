{
  "name": "violinist-dev/allow-list-handler",
  "description": "Handles the allow list for violinist",
  "type": "library",
  "license": "MIT",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Eirik S. Morland",
      "email": "eirik@morland.no"
    }
  ],
  "autoload": {
    "psr-4": {
      "Violinist\\AllowListHandler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Violinist\\AllowListHandler\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "config": {
    "platform": {
      "php": "7.0.0"
    }
  },
  "scripts": {
    "test": "@composer lint && phpunit",
    "lint": "phpcs -p -n"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3.4",
    "php-coveralls/php-coveralls": "^2.4",
    "wa72/simplelogger": "^1.1"
  },
  "require": {
    "violinist-dev/violinist-config": "^1.2.0",
    "psr/log": "^1.1"
  }
}
